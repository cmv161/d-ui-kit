{
    "presets": [
        ["@babel/preset-env", { "targets": { "ie": "11" } }],
        "@babel/preset-react",
        "@babel/preset-typescript",
        "minify"
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime", { "regenerator": true }],

        [
            "css-modules-transform",
            {
                "generateScopedName": "[folder]__[local]--[hash:base64:5]",
                "extractCss": "./lib/css/styles.css"
            }
        ]
    ]
}